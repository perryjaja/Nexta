<odoo>
    <record id="product_template_barcode_form_view_custom" model="ir.ui.view">
        <field name="name">product.template.barcode.form.view.custom</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
             <xpath expr="(//field[@name='description'])" position="after">
                <field name="cod_barcode_prod"/>
            </xpath>
        </field>
    </record>
</odoo>